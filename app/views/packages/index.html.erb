<h5 class="title">List of available packages</h5>

<% @packages.each do |package| %>
  <h4>
    <%= link_to package.name, package %>
    <small>
      version: <%= package.version %>
    </small>
  </h4>
  <% if owner(package) %>
    <p>
      <%= link_to 'edit', edit_package_path(package) %> |
      <%= link_to 'destroy', package, method: :delete, data: { confirm: 'Are you sure?' } %>
    </p>
  <% end %>
<% end %>
